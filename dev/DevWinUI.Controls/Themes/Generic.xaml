                        <Grid x:Name="TabContainer" Grid.Column="1" Padding="{ThemeResource TabViewItemHeaderPadding}" Background="{ThemeResource TabViewItemHeaderBackground}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Control.IsTemplateFocusTarget="True" CornerRadius="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CornerRadius}" FocusVisualMargin="{TemplateBinding FocusVisualMargin}">
