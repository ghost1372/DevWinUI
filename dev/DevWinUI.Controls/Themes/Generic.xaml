                    <local:HaloRingPanel>
                        <Grid local:HaloRingPanel.Angle="{Binding Angle, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}" local:HaloRingPanel.Offset="{Binding Offset, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
                            <ContentControl Width="{TemplateBinding local:Halo.Thickness}"
                                            Height="{TemplateBinding local:Halo.Thickness}"
                                            Background="{TemplateBinding Background}"
                                            Template="{TemplateBinding Thumb}" />
                        </Grid>
                    </local:HaloRingPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
