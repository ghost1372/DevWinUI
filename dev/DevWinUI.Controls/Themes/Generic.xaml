                                        <Setter Target="TabContainer.BorderBrush" Value="{ThemeResource TabViewItemRoundedSelectedBorderBrush}" />
                                        <Setter Target="TabContainer.BorderThickness" Value="{ThemeResource TabViewItemRoundedSelectedBorderThickness}" />
